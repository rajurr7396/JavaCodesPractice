package aeroplanes;

public class aeroplanes {
	
	int cost;
	private int speed;
	
	
	public void Start() {
		System.out.println("Aeroplanes started");
	}
	
	public int getCost() {
		return cost;
	}
	
	public void setCost(int cost) {
		if (cost> 1) {
		this.cost = cost;
	}
	}
	
	public void setIncrease(int costincrease) {
		if (this.cost > 1) {
		setCost(this.cost + costincrease);
	}
	}
	
	public int getSpeed() {
		return speed;
	}
	
	public void setSpeed(int speed) {
		this.speed = speed;
	}
	
	
	
	
	
	

}
