package aeroplanes;

public class aeroplanerunner {

	public static void main(String[] args) {
		aeroplanes obj = new aeroplanes();
		
		obj.Start();
		
		obj.setCost(-90);
		obj.setSpeed(900);
		
		int r = obj.getCost();
		int y = obj.getSpeed();
		
		System.out.println(r + " " + y);
		
		obj.setIncrease(1000);
		int i = obj.getCost();
		System.out.println(i + " " + y);


	}

}
