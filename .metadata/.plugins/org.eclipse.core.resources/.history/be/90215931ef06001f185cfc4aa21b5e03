
package motors;

public class motors {
	int speed;
	private int velocity;
	
	void setVelocity(int velocity) {
		if (velocity > 0) {
		this.velocity = velocity;
	}
	}
	
	int getVelocity() {
		return velocity;
	}
	
	
	
    void start() {
    	System.out.println("started");
    }

	public void setIncrease(int change) {
		setVelocity(this.velocity + change);
	}
		
	}

	
