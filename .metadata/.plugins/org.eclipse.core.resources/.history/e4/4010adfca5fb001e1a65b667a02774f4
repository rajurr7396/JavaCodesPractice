package com.example.mypackage;

class pavan {
	
	public double show(double a, int b) {
		return a + b;
	}
 
}

public class raju {
    public static void main(String[] args) {
    	
    	double a = 6;
    	int b = 8;
    	
    	pavan obj = new pavan();
    	double result = obj.show(a, b);
    	System.out.println("Result is " +result);
       
    }
}
