package switcho;

public class switcho {

    public static void main(String[] args) {
        int x = 9;

        switch (x) {
            case 1: {
                System.out.println("r");
                break;
            }
            case 2: {
                System.out.println("8");
                break;
            }
            case 3: {
                System.out.println("i");
                break;
            }
            case 9: {
                System.out.println("t");
                break;
            }
            case 8: {
                System.out.println("k");
                break;
            }
            case 7: {
                System.out.println("s");
                break;
            }
        }
    }
}
